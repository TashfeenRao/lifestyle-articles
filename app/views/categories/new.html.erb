<div class="container">
    <div class="row">
        <%= form_for(@category) do|f|%>
        <div class="col-md-12">
            <%= render 'shared/errors', object: f.object%>
            <%= f.text_field :name, placeholder:"Category Name"%>
            <%= f.number_field :priority, placeholder:"priority"%>
        </div>
        <div class="col-md-12">
            <%= f.submit "ADD", class:"btn btn-primary"%>
        </div>
        <%end%>
    </div>
</div>